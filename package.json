{
  "name": "@travi/travi.org-core-components",
  "description": "core React components for Travi.org sites",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "sideEffects": false,
  "files": [
    "lib/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "travi-org/core-components",
  "bugs": "https://github.com/travi-org/core-components/issues",
  "homepage": "https://npm.im/@travi/core-components",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "lint:lockfile": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm",
    "lint:js": "eslint . .storybook/ --cache",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "clean": "rimraf ./lib",
    "prebuild": "run-s clean",
    "build": "npm-run-all --print-label --parallel build:*",
    "build:js": "rollup --config",
    "build:storybook": "build-storybook --quiet",
    "predeploy:netlify": "run-s build:storybook",
    "deploy:netlify": "netlify deploy --dir=./storybook-static",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "start": "start-storybook --port 8888 --ci",
    "test:unit": "nyc run-s test:unit:base",
    "test:unit:base": "DEBUG=any mocha 'src/**/*-test.js'",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint:peer": "npm ls >/dev/null",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@material-ui/core": "^4.8.2",
    "@material-ui/icons": "^4.5.1",
    "@travi/brand": "^1.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/register": "7.12.1",
    "@emotion/babel-preset-css-prop": "10.2.1",
    "@storybook/react": "6.0.28",
    "@travi/any": "2.0.15",
    "@travi/babel-preset": "3.0.38",
    "@travi/eslint-config": "1.0.59",
    "@travi/eslint-config-mocha": "1.0.8",
    "@travi/eslint-config-react": "1.0.53",
    "babel-loader": "8.2.2",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "ban-sensitive-files": "1.9.14",
    "chai": "4.2.0",
    "codecov": "3.8.1",
    "commitlint-config-travi": "1.3.11",
    "cz-conventional-changelog": "3.3.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "husky": "7.0.4",
    "lockfile-lint": "4.3.7",
    "mocha": "8.2.1",
    "netlify-cli": "2.65.6",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "remark-cli": "9.0.0",
    "remark-preset-lint-travi": "1.3.11",
    "rimraf": "3.0.2",
    "rollup": "2.38.0",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "sinon": "9.2.4",
    "webpack": "4.44.1"
  }
}
